<article class="el-page-content">
    <h1 style="max-width: 600px;">{title}</h1>
    <p>{introduction}</p>
    <h2 style="margin-top: 80px; margin-bottom: 70px;">{title_list}</h2>
    <!-- BEGIN LIST -->
    <ul class="el-list-authors">
        <!-- BEGIN BLOCK_AUTHOR -->
        <li>
            <img src="{image_src}" alt="{image_title}">
            <p>{image_title}</p>
        </li>
        <!-- END BLOCK_AUTHOR -->
    </ul>
    <!-- END LIST -->
</article>
<article class="el-page-content">
    <h1>{title_brand}:</h1>
    <ul class="el-list-partners">
        <!-- BEGIN BRAND -->
        <li>
            <img src="{brand_image_src}" alt="{brand_image_title}" />
        </li>
        <!-- END BRAND -->
    </ul>
</article>